{
  "name": "phaset-action",
  "description": "Integrates your data with Phaset",
  "version": "1.0.0",
  "author": "Phaset",
  "keywords": [
    "phaset",
    "dora",
    "dora-metrics",
    "devops",
    "software-delivery",
    "linter",
    "audit",
    "test-automation",
    "standards",
    "compliance",
    "auditor",
    "engops"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phasetdev/phaset-action"
  },
  "bugs": {
    "url": "https://github.com/phasetdev/phaset-action/issues"
  },
  "homepage": "https://github.com/phasetdev/phaset-action-action",
  "scripts": {
    "test": "bats test.bats",
    "deploy": "bash phaset.sh --org-id \"demoorg\" --record-id \"demorecord\" --token \"abc123\" --endpoint \"https://phaset.example.com\" --action \"deployment\"",
    "standards": "bash phaset.sh --org-id \"demoorg\" --record-id \"demorecord\" --token \"abc123\" --endpoint \"https://phaset.example.com\" --action \"standards\"",
    "record": "bash phaset.sh --org-id \"demoorg\" --record-id \"demorecord\" --token \"abc123\" --endpoint \"https://phaset.example.com\" --action \"record\"",
    "update:v1": "git tag -d v1 && git push origin -d v1 && git tag -a \"v1\" -m \"v1\" && git push origin v1"
  },
  "dependencies": {
    "standardlint": "latest"
  }
}